heartCog <- function(x) { list(
meanMLII = cogMean(x$MLII),
RangeMLII = cogRange(x$MLII),
nObs = cog(sum(!is.na(x$MLII)),
desc = "number of sensor readings"),
meanV5 = cogMean(x$V5),
RangeV5 = cogRange(x$V5)
)}
